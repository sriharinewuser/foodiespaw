{
  "version": 2,
  "builds": [
    {
      "src": "frontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/frontend/build/$1"
    },
    {
      "src": "/",
      "dest": "/frontend/build/index.html"
    }
  ],
  "buildCommand": "npm run build",
  "installCommand": "npm install"
}